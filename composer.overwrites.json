{
  "repositories": [
    {
      "type": "path",
      "url": "../Game",
      "options": {
        "versions": {
          "opentribes/core": "0-dev"
        }
      }
    }
  ],
  "require": {
    "symfony/webpack-encore-bundle": "^1.14",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11.0",
    "opentribes/core": "^0@dev",
    "sensio/framework-extra-bundle": "^6.2",
    "sulu/community-bundle": "^2.0@dev",
    "symfony/mailer": "^5.4",
    "doctrine/dbal": "^3.2"
  },
  "require-dev": {
    "nunomaduro/phpinsights": "^2",
    "vimeo/psalm": "^4"
  },
  "scripts": {
    "ot:phpinsights": "vendor/bin/phpinsights --config-path=phpinsights.php",
    "ot:fix-style": "vendor/bin/phpinsights --config-path=phpinsights.php -s --fix"
  }
}